                <TableColumn fx:id="colunaPrato" prefWidth="126.0" text="Nome" />
                <TableColumn fx:id="colunaCodPrato" prefWidth="60.0" text="Código" />
                <TableColumn fx:id="colunaDescPrato" prefWidth="459.0" text="Descrição" />
                <TableColumn fx:id="colunaStatus" prefWidth="77.0" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="botaoSituacao" layoutX="14.0" layoutY="276.0" mnemonicParsing="false" onAction="#acaoFinalizar" prefHeight="25.0" prefWidth="134.0" style="-fx-background-color: #00b33c;" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
      <Label fx:id="labelStatus" layoutX="250.0" layoutY="279.0" prefHeight="19.0" prefWidth="223.0" textFill="#d32828">
